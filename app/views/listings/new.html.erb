<h1>New Listing!</h1>

<form action= <%=user_listings_url%> method= "post">

  <input type="hidden"
  name="authenticity_token"
  value="<%= form_authenticity_token %>">


  <label for= "listing_title">Title for listing:</label>
  <input type= "text" name= "listing[title]" id="listing_title">

  <label for= "listing_start_date">Start Date:</label>
  <input type= "date" name= "listing[start_date]" id= "listing_start_date">

  <label for="listing_end_date">End Date:</label>
  <input type= "date" name= "listing[end_date]" id= "listing_end_date">

  <label for="listing_street_address">Street Address:</label>
  <input type="text" name="listing[street_address]" id="listing_street_address">

  <label for="listing_city">City</label>
  <input type="text" name="listing[city]" id="listing_city">

  <label for="listing_zipcode">Zipcode</label>
  <input type="text" name="listing[zipcode]" id="listing_zipcode">

  <label for="listing_country">Country:</label>
  <input type="text" name="listing[country]" id="listing_country">

  <label for="listing_details">Description:</label>
  <textarea name="listing[details]" id= "listing_details" rows="8" cols="40"></textarea>

  <input type= "submit" value= "Post Listing!">


</form>
