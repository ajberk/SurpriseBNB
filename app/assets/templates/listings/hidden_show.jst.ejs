<div class="jumbotron">
  <h1>How About Here?</h1>
</div>

<div class="main-img-div"></div>

<div class="col-md-6">
  <br>
  <div class="list-group">
    <a class="list-group-item active">
      <h1 class="list-group-item-heading"><%= listing.escape("title") %></h1>
    </a>
    <a class="list-group-item">
      <h1 class="list-group-item-heading">Price</h1>
      <p class="list-group-item-text"><%= listing.escape("price") %> Dollars A Night</p>
    </a>
    <a class="list-group-item">
      <h1 class="list-group-item-heading">Details</h1>
      <p class="list-group-item-text"><%= listing.escape("details") %></p>
    </a>
  </div>

  <div class="list-group">
    <a class="list-group-item active">
      <h3 class="list-group-item-heading">Comments</h3>
    </a>
    <div class="comments"></div>

  </div>
  <div class="list-group">
    <br>
    <div class="list-group">
      <a class="list-group-item active">
        <h3 class="list-group-item-heading">Add a Comment</h3>
      </a>
    </div>
    <div class="comment-form"></div>
  </div>
</div>

<div class="col-md-6">

  <br>
  <input id= "book-listing" type="submit" class="btn btn-primary" value="Book It!" method= "get" class="form-control">
  <br>

  <div id = "mygallery" class="image-div"></div>
  <div class="jumbotron jumbo-small">
    <h2 >Not For You? Search Again!</h2>
  </div>
  <div class= "search-form"></div>
</div>
